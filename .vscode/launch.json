{
    "configurations": [
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: 配置项目",
            "cmakeDebugType": "configure",
            "clean": false,
            "configureAll": false
        },
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: 脚本调试",
            "cmakeDebugType": "script",
            "scriptPath": "${workspaceFolder}/<...>.cmake"
        },
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake: 外部启动",
            "cmakeDebugType": "external",
            "pipeName": "<...>"
        },
        {
            "processId": 0,
            "name": "(Windows) 附加",
            "type": "cppvsdbg",
            "request": "attach"
        },
        {
            "name": "(Windows) 启动",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "输入程序名称，例如 ${workspaceFolder}/a.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal"
        },
        {
            "type": "cmake",
